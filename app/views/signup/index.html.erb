<link rel="stylesheet" href="/../assets/stylesheets/signup.css">
<%= stylesheet_link_tag "application" %>
<%= form_with scope: :signup, local: true do |form| %>
<p>
  <%= form.label :username %>
  <%= form.text_field :username %>
</p>
<p>
  <%= form.label :password %>
  <%= form.text_field :password %>
</p>
<p>
  <%= form.label :email %>
  <%= form.text_field :email %>
</p>
<p  class = "Submit">
  <%= form.submit "Submit" %>
</p>
<% end %>
